<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>loginServlet</servlet-name>
  	<display-name>loginServlet</display-name>
  	<description></description>
  	<servlet-class>org.daijia.Servlet.loginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>querydaijiaServlet</servlet-name>
  	<display-name>querydaijiaServlet</display-name>
  	<description></description>
  	<servlet-class>org.daijia.Servlet.queryallServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>queryOneServlet</servlet-name>
  	<display-name>queryOneServlet</display-name>
  	<description></description>
  	<servlet-class>org.daijia.Servlet.queryOneServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OderServlet</servlet-name>
  	<display-name>OderServlet</display-name>
  	<description></description>
  	<servlet-class>org.daijia.Servlet.OderServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>applyServlet</servlet-name>
  	<display-name>applyServlet</display-name>
  	<description></description>
  	<servlet-class>org.daijia.Servlet.applyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginServlet</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>querydaijiaServlet</servlet-name>
  	<url-pattern>/queryallServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>queryOneServlet</servlet-name>
  	<url-pattern>/queryOneServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OderServlet</servlet-name>
  	<url-pattern>/OderServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>applyServlet</servlet-name>
  	<url-pattern>/applyServlet</url-pattern>
  </servlet-mapping>
</web-app>
